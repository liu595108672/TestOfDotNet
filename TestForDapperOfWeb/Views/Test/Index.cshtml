@model List<Person>
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Age</td>
        </tr>
    }
</table>
